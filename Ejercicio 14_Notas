#include <iostream> 

using namespace std;

int main(int argc, char const *argv[])
{
    int nota = 0;

    cout << "Ingrese la nota que obtuvo: ";
    cin  >> nota;

    if ( nota >= 95 && nota <=100 ) {
       cout << "Obtuvo una A ";
    } else {
        if ( nota >= 85 && nota <= 94) {
            cout << "Obtuvo una B ";
        } else {
            if ( nota >= 75 && nota <= 84) {
                 cout << "Obtuvo una C ";
            } else {
                  if ( nota >=65 && nota <= 74) {
                      cout << "Obtuvo una D ";
                  } else {
                       if ( nota >= 60 && nota <= 64) {
                           cout << "Obtuvo una E ";
                       } else {
                             if (nota >=0 && nota <= 60) {
                                cout << "Obtuvo una F ";
                            }
                       }
                  }
            }
        }
    }   

    return 0;
}
